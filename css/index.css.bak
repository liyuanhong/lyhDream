body{
	height:100%;
	overflow:hidden;
	margin:0px;
	padding:0px;
}
li{
	list-style-type:none;
	display:inline;	
	padding-right:10px;
	cursor:pointer;
}
h1{
	margin:0px;
}



#div_1{
	background-color:#5C6282;
	height:100px;
	width:100%;
	margin:0px;
	padding-top:20px;
}
#div_2{
	background-color:#CB7953;
	height:500px;
	width:100%;
	margin:0px;
}
#title{
	background-color:#5C6282;
	margin-top:0px;
	text-align:center;
	color:#FD5C01;
}
#login_module{
	float:right;
	margin-right:50px;
}
#head_portrait{
	float:left;
	margin-left:200px;
	border-color:#ffffff;
	margin-top:10px;
	cursor:pointer;
}
#fullbg{
	background-color:gray; 
	left:0; 
	opacity:0.5; 
	position:absolute; 
	top:0; 
	z-index:3; 
	filter:alpha(opacity=50); 
	-moz-opacity:0.5; 
	-khtml-opacity:0.5;
}
#login_window{
	background-color:#fff; 
	border:5px solid rgba(0,0,0, 0.4); 
	height:400px; 
	left:50%; 
	margin:-200px 0 0 -200px; 
	padding:1px; 
	position:fixed !important; /* ¸¡¶¯¶Ô»°¿ò */ 
	position:absolute; 
	top:50%; 
	width:400px; 
	z-index:5; 
	border-radius:5px; 
	display:none;
}
#dialog p { 
margin:0 0 12px; 
height:24px; 
line-height:24px; 
background:#CCCCCC; 
} 
#dialog p.close { 
text-align:right; 
padding-right:10px; 
} 
#dialog p.close a { 
color:#fff; 
text-decoration:none; 
} 



.user_info_nic{
	font-weight:bold;
	float:left;
	margin-top:15px;
	margin-left:10px;
}